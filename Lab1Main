import sys
from PyQt6.QtWidgets import QApplication
from gui.main_window import MainWindow
from controllers.lab1_controller import Lab1Controller


def main() -> None:
  app = QApplication(sys.argv)
  view = MainWindow()
  controller = Lab1Controller(view)
  view.show()
  sys.exit(app.exec())


if __name__ == "__main__":
  main()
